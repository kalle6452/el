import numpy as np
import matplotlib.pyplot as plt
#x = np.array([1000, 8000, 100, 10, 2500, 5000])
x = np.array([500,1000,2000,3000,4000,5000,10000,15000,20000,30000,40000,50000])
y = np.array([-90, -72, -64.8, -43.2, -26.5, 0, 50.4, 67.5, 72, 78.3, 82.8, 85.5])
#plt.subplot(2,1,1)
plt.scatter(x,y)
plt.plot(x,y)

#plt.yscale('log')
plt.xscale('log')
plt.show()

